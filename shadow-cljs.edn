;; .shadow-cljs.edn

{:source-paths
 ["src"]

 :dependencies
 [[org.clojure/clojurescript "1.11.132"]
  [thheller/shadow-cljs "2.28.18"]
  [quil "4.3.1563"]]

 :builds
 {:app
